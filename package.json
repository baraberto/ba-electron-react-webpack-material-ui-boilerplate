{
  "name": "ba-electron-react-webpack-material-ui",
  "version": "0.1.0",
  "description": "Electron, React, PostCSS e Webpack para começar o seu próximo mambo",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/baraberto/ba-electron-react-webpack-material-ui-boilerplate.git"
  },
  "homepage": "",
  "bugs": {
    "url": "https://github.com/baraberto/ba-electron-react-webpack-material-ui-boilerplate.git/issues"
  },
  "author": {
    "name": "David Miguel",
    "email": "dvdmgl@gmail.com",
    "url": "https://github.com/dvdmgl"
  },
  "keywords": [
    "app",
    "boilerplate",
    "electron",
    "eslint",
    "flow",
    "open",
    "open-source",
    "postcss",
    "prettier",
    "react",
    "reactjs",
    "source",
    "webpack"
  ],
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=5.0.0"
  },
  "main": "main.js",
  "scripts": {
    "prod": "webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "webpack-dev-server --hot --host 0.0.0.0 --port 9147 --config=./webpack.dev.config.js --mode development",
    "build": "webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./builds"
  },
  "dependencies": {
    "@hapi/b64": "^4.2.1",
    "@material-ui/core": "4.3.3",
    "@material-ui/icons": "4.2.1",
    "@material-ui/styles": "4.3.3",
    "@material-ui/system": "4.3.3",
    "axios": "0.19.0",
    "clsx": "1.0.4",
    "dotenv": "8.1.0",
    "file-type": "12.2.0",
    "lodash": "4.17.15",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.1",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "read-chunk": "3.2.0",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "sharp": "^0.23.0",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
